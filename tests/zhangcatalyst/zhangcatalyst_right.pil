# Simplified entropy-driven catalyst system used to test p_R, the probability of successful dissociation on the left during Reactions 2 and 3

# PUBLISHED SEQUENCES
#sequence d1 = CTTTCCTACA : 10
#sequence d2 = CCTACGTCTCCAACTAACTTACGG : 24
sequence t3 = CCCT : 4
sequence d4 = CATTCAATACCCTACG : 16
sequence t5 = TCTCCA : 6
#sequence d6 = CCACATACATCATATT : 16

strand F_mod = t3 d4 : 20 # Remove left domains for simplicity
strand C = d4 t5 : 22
strand LB_mod = t5* d4* t3* : 26 # Remove left domains for simplicity
#strand OB = d1 d2 : 34
#strand SB = d6 t3 d4 : 36

structure Fuel = F_mod : ..
structure Intermediate = C + LB_mod : ((+)). # Removed left domains/strands
structure Right = C : .. # Strand removed during successful dissociation
structure RightDissoc = F_mod + LB_mod : ((+.)) # Complex after successful dissociation
