# Peppercorn paper, multiple fates system
sequence dA = TTCTCTTTATTTCCC : 15
sequence dB = GCTCCATCCCTTCAC : 15
sequence dX1 = GCTTGGCCGACGTCC : 15
sequence dX2 = GCACGTCAGCGTCCG : 15
sequence dR_AB = TTTATATTTCGGCCT : 15
sequence t1 = CCTTTC : 6
sequence t2 = ACGTTG : 6

strand base1 = t2 dX1* t1 : 0
strand base2 = t2 dX2* t1 : 0
strand sigA = dA dX1 t2* : 0
strand sigB = dB dX2 t2* : 0
strand rxnAB = t1* dX1 t1* dX2 dR_AB : 0
strand rxnAB_wasteB = dX2* dB* : 0
strand rxnAB_wasteAB = dB t1 dX1* dA* t1 : 0
strand rxnAB_wasteA = t1* dA : 0

structure SigA = base1 + sigA : ((.+.))
structure SigB = base2 + sigB : ((.+.))
structure RxnAB = rxnAB + rxnAB_wasteB + rxnAB_wasteAB + rxnAB_wasteA : .(((.+)(+)))((+))
structure RxnAB_WasteA = rxnAB_wasteA : ..

structure RxnAB_SigA_1 = base1 + rxnAB + rxnAB_wasteB + rxnAB_wasteAB + sigA : (((+)(((.+)(+)))(.+)))
structure RxnAB_SigA_2 = base1 + rxnAB + rxnAB_wasteB + rxnAB_wasteAB + sigA : (((+))((.+)(+))((.+)))
structure RxnAB_SigA_3 = base1 + rxnAB + rxnAB_wasteB + rxnAB_wasteAB + sigA : .((+)).(.+)(+).((.+)).
structure RxnAB_WasteAB = rxnAB_wasteAB + sigA : ..((.+)).

structure RxnAB_SigB_1 = base1 + rxnAB + rxnAB_wasteB + sigB + base2 : .((+))((.+)(+)((+)))
structure RxnAB_SigB_2 = base1 + rxnAB + rxnAB_wasteB + sigB + base2 : .((+))((.+((+))(+)))
structure RxnAB_WasteB = rxnAB_wasteB + sigB : ((+)).
structure RxnAB_Base = base1 + rxnAB + base2 : .((+))((.+.))
