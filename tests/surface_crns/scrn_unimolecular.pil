# Peppercorn paper, multiple fates system
sequence dA = CATCTCATTCTAAAC : 14
sequence dX = CTTCTACTTCATAAC : 14
sequence dR_A = CATTTCAACTAAACC : 14
sequence dB = CTTACATTTCAACAC : 14
sequence t1 = CTCCTC : 6
sequence t2 = ACACAC : 6

strand base = t2 dX* t1 : 0
strand sigA = dA dX t2* : 0
strand sigB = dB dX t2* : 0
strand rxnA = t1* dX dR_A : 0
strand sigA_rxnA_1 = dX* dA* : 0
strand sigA_rxnA_2 = dA : 0
strand rxnA_sigB_1 = dR_A : 0 
strand rxnA_sigB_2 = dR_A* dX* : 0

structure SigA = base + sigA : ((.+.))
structure A_RxnA = rxnA + sigA_rxnA_1 + sigA_rxnA_2 : .(.+)(+)
structure RxnA = base + rxnA : .((+)).
structure RxnA_B = sigB + rxnA_sigB_1 + rxnA_sigB_2 : .(.+(+))
structure SigB = base + sigB : ((.+.))
structure WasteA = sigA_rxnA_2 : .
structure WasteA_RxnA = sigA + sigA_rxnA_1 : ((.+))
structure WasteRxnA = rxnA_sigB_1 : .
structure WasteRxnA_B = rxnA + rxnA_sigB_2 : .((+))
